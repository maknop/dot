# Command 'ls' shows all files including dotfiles
# alias ls='ls -A'
# alias ll='ls -alF'
# Moving a file gives an explanation
alias mv='mv -v'

# Creating a directory gives explanation
alias mkd='mkdir -pv'

# Running python command is always version >=3
alias python='python3'

# Pip
alias pip='pip3'

# Git
alias ga='git add'
alias gc='git commit -m'
alias gp='git push'
alias gs='git status'

# vim
alias v='vim'

# Java programming, compile & run file
alias jc='javac'
alias j='java'

# Docker
alias up='docker-compose up'
alias build='docker-compose build'
alias upbuild='docker-compose up -d --build'

# tmux
alias t='tmux'
alias tl='tmux ls'
alias ta='tmux attach-session -t' # <session-name>
alias tk='tmux kill-session -t' 
alias ts='tmux new -s' # <session-name> 

# Access to cloud server
alias myserver=${MYSERVER}
